<Window x:Class="SnakeWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnakeWPF"
        mc:Ignorable="d"
        Title="Simple WPF Snake" Height="600" Width="400" SizeToContent="WidthAndHeight" KeyUp="OnKeyUp">
    <Border BorderBrush="White" BorderThickness="1">
        <Canvas Name="GameArea" ClipToBounds="True" Width="400" Height="600" Background="Black">
        </Canvas>
    </Border>
</Window>
